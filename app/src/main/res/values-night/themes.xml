<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.DashCoreGroupChallenge" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="android:windowBackground">@color/window_background</item>

        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryVariant">@color/black</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/colorAccent3</item>
        <item name="colorSecondaryVariant">@color/colorAccent3</item>
        <item name="colorOnSecondary">@color/colorPrimary</item>

        <item name="colorAccent">@color/colorPrimary</item>

        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
    <style name="Theme.DashCoreGroupChallenge.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.DashCoreGroupChallenge.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.DashCoreGroupChallenge.PopupOverlay" parent="ThemeOverlay.AppCompat.Light">
        <item name="windowNoTitle">false</item>
    </style>
</resources>